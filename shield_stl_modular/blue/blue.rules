	// On
	
	ToggleComponents_Blue_On
	{
		Type = ToggledComponents
		Toggle = Toggle_Blue_On
		IncludeInBlueprints = false
		Components : <../purple/purple.rules>/Components/ToggleComponents_Purple_On/Components
		{
			// Inner
			ArcShield_InnerFront :		^/0/ArcShield_InnerFront
			{
				ShieldMediaEffects
				[
					: &../^/0/ShieldMediaEffects/0
					{
						ArcSprite : &<../../colors.rules>/Color/Blue/ShieldMediaEffect_On, ^/0/ArcSprite	{}
					}
					: &../^/0/ShieldMediaEffects/1	{}
				]
				HitMediaEffects
				[
					: &../^/0/HitMediaEffects/0
					{
						ArcSprite : &<../../colors.rules>/Color/Blue/HitMediaEffects, ^/0/ArcSprite	{}
					}
					: &../^/0/HitMediaEffects/1 {}
				]
			}
			ArcShield_InnerFrontRight :	<../stats.rules>/Stats/ArcShield_InnerFrontRight0, ArcShield_InnerFront	{}
			ArcShield_InnerBackRight :	<../stats.rules>/Stats/ArcShield_InnerBackRight0, ArcShield_InnerFront	{}
			ArcShield_InnerBack :		<../stats.rules>/Stats/ArcShield_InnerBack0, ArcShield_InnerFront		{}
			ArcShield_InnerBackLeft :	<../stats.rules>/Stats/ArcShield_InnerBackLeft0, ArcShield_InnerFront	{}
			ArcShield_InnerFrontLeft :	<../stats.rules>/Stats/ArcShield_InnerFrontLeft0, ArcShield_InnerFront	{}

			// Always
			STL_EmitterEffect : ^/0/STL_EmitterEffect
			{
				OperationalToggle = IsOperational
				MediaEffects
				[
					: &../^/0/MediaEffects/0
					{
						ArcSprite : &<../../colors.rules>/Color/Blue/ShieldMediaEffect_On, ^/0/ArcSprite	{}
					}
					: &../^/0/MediaEffects/1	{}
				]
			}
		}
	}

