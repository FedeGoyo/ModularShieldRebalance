			{
				BaseValue = 0
				Modifiers
				[
					{
						Type = Status
						StatusType = cosmoteer.shield_overload
						ModificationMode = Subtract
					}
				]
			}
		}
		InnerBatteryStorageProxy	: BatteryStorageProxy	{ ComponentID = InnerBatteryStorage }	// For Modular, to prevent using it while PowerToggle is off.
		
		OuterBatteryStorage : InnerBatteryStorage	{}
		OuterBatteryStorageProxy	: BatteryStorageProxy	{ ComponentID = OuterBatteryStorage }	// For Modular, to prevent using it while PowerToggle is off.

